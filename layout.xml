<?xml version="1.0" encoding="utf-8"?>
<layout onLaunch="launch" onVolumeUp="volume_up" onVolumeDown="volume_down" onResume="resume" onPause="pause" lightcolor="#e77300" darkcolor="#c46e19">
  <grid>
    <row>
      <button text="Brightness Down" onTap="brightness_down" />
      <button icon="fullscreen" ontap="fullscreen" />
      <button text="Brightness Up" onTap="brightness_up" />
    </row>
    <row>
      <button icon="vdown" onTap="volume_down" />
      <button icon="vmute" onTap="volume_mute" />
      <button icon="vup" onTap="volume_up" />
    </row>
    <row>
      <button icon="rwd" onTap="rewind" />
      <button icon="playpause" onTap="play_pause" />
      <button icon="ff" onTap="fast_forward" />
    </row>
    <row>
      <button text="1X" ontap="playback1" />
      <button text="2X" ontap="playback2" />
      <button text="4X" ontap="playback4" />
    </row>
  </grid>
</layout>
